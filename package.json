{
  "devDependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "flex-gap-polyfill": "^2.2.1",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-gutters": "^3.0.1",
    "tailwindcss": "^2.0.2"
  },
  "scripts": {
    "build": "set NODE_ENV=production&&postcss app/static/css/tailwind.css -o app/static/style.css",
    "dev": "postcss tailwind.css -o style.css "
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "discardComments": {
          "removeAll": true
        }
      }
    ]
  },
  "dependencies": {
    "tailwindcss-neumorphism": "^0.1.0"
  }
}
